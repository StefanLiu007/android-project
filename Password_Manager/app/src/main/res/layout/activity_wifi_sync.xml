<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:toopbar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.WifiSyncClientActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/base_blue"
        toopbar:navigationIcon="@drawable/arrow_left"
        toopbar:popupTheme="@style/AppTheme.PopupOverlay"
        toopbar:theme="@style/AppTheme.AppBarOverlay"
        toopbar:title="Wifi Method"
        toopbar:titleTextColor="@color/white"></android.support.v7.widget.Toolbar>

    <!--<include layout="@layout/sync_conn_des_layout" />-->

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/padding"></android.support.v7.widget.RecyclerView>
</LinearLayout>
